#include <bits/stdc++.h>
using namespace std;
int main() {
    // cout<<"Hello World!";
    int a[5]={2,3,4,5,6};
    int b[6]={3,1,3,44,67,71};
    set<int> s;
    cout<<"Array 1 : ";
    for(int i=0;i<5;i++)
    {
        s.insert(a[i]);
        cout<<a[i]<<" ";
    }
     cout<<"\nArray 2 : ";
    for(int i=0;i<6;i++)
    {
        cout<<b[i]<<" ";
        s.insert(b[i]);
    }
    cout<<"\nUnion of array is : ";
    for(auto it:s)
    {
        cout<<it<<" ";
    }
}
